algoritmo "procedimentosoma"
var
   x, y: inteiro
procedimento soma(a,b: inteiro) //procedimento (parametro)
inicio
      escreval("Recebi o valor", a)
      escreval("Recebi o valor", b)
      escreval("A soma entre os valores e", a+b)
fimprocedimento
inicio
      x <- 5
      y <- 3
      soma (x, y)
fimalgoritmo