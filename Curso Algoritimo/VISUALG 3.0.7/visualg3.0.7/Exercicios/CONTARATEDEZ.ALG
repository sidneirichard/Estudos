algoritmo "contaratedez"
var
   contador, valor, salto: inteiro
inicio
      escreva(" quer contar ate quanto?: ")
      leia(valor)
      escreva(" qual sera o seu salto?: ")
      leia(salto)
      contador <-0
      enquanto (contador <= valor)faca
         contador <- (contador + salto)
         escreval(contador)
      fimenquanto
      escreval(" Terminei de contar. ")
fimalgoritmo